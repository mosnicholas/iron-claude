{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "morning-notify.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/index.ts"
    },
    {
      "src": "/api/cron/morning",
      "dest": "/morning-notify.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/morning",
      "schedule": "0 13 * * 1-5"
    }
  ]
}
