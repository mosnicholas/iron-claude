{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "",
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/cron/daily-reminder",
      "methods": ["GET"],
      "dest": "/api/cron/daily-reminder.ts"
    },
    {
      "src": "/api/cron/weekly-retro",
      "methods": ["GET"],
      "dest": "/api/cron/weekly-retro.ts"
    },
    {
      "src": "/api/cron/weekly-plan",
      "methods": ["GET"],
      "dest": "/api/cron/weekly-plan.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-reminder",
      "schedule": "0 11 * * 1-5"
    },
    {
      "path": "/api/cron/weekly-retro",
      "schedule": "0 23 * * 6"
    },
    {
      "path": "/api/cron/weekly-plan",
      "schedule": "0 1 * * 0"
    }
  ]
}
